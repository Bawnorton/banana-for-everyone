{
  "schemaVersion": 1,
  "id": "banana-plus",
  "version": "${version}",
  "name": "Banana Plus",
  "description": "This addon is literally immortal.",
  "authors": [
    "RickyTheRacc",
    "CrytoPal"
  ],
  "contact": {
    "repo": "https://github.com/Bennooo/banana-for-everyone"
  },
  "icon": "assets/logo1.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "bananaplus.BananaPlus"
    ]
  },
  "mixins": [
    "meteorclient.mixins.json",
    "minecraft.mixins.json"
  ],
  "accessWidener": "banana.accesswidener",
  "custom": {
    "meteor-client:color": "255,193,0"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=1.19.1",
    "meteor-client": "*"
  },
  "breaks": {
    "bedtrap": "*",
    "splashhack": "*",
    "deltahack": "*"
  }
}
